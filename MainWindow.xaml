<Window x:Class="PR3.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PR3"
        mc:Ignorable="d"
        Title="MainWindow" Height="200" Width="200" WindowStartupLocation="CenterScreen" ResizeMode="NoResize">
    <Grid Background="#FF6CA9A5">
        <Label Content="Количество игроков" Foreground="White" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="0, 30, 0, 0" FontSize="14"/>
        <TextBox Name="count" Height="20" Width="74" Background="White" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="0, 65, 0, 0"/>
        <Button x:Name="start" Content="Начать" Height="20" Background="White" HorizontalAlignment="Center" VerticalAlignment="Top" Click="start_Click" Width="54" Margin=" 0, 110, 0, 0" BorderBrush="White"/>
    </Grid>
</Window>
